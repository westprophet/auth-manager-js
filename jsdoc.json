{
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown", "better-docs/component", "node_modules/better-docs/category", "node_modules/better-docs/typescript"],
  "templates": {
    "better-docs": {
      "name": "Analytics Docs"
    }
  },
  "opts": {
    "template": "./node_modules/better-docs",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
